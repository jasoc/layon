{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
    },
    "overrides": [
        {
            "files": ["*.ts"],
            "rules": {
                "indent": ["error", 2],
                "max-len": [2, {"code": 100, "tabWidth": 4, "ignoreUrls": true}],
                "require-jsdoc": "off",
                "new-cap": "off"
            }
        }
    ]
}
