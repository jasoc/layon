{
    "version": "0.2.0",
    "configurations": [
      {
        "name": ".NET Core Launch (console)",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "${workspaceFolder}/layon-backend/bin/Debug/net5.0/layon-backend.dll",
        "args": [],
        "cwd": "${workspaceFolder}",
        "stopAtEntry": false,
        "console": "internalConsole"
      },
      {
        "name": "Launch ng serve & Chrome",
        "type": "chrome",
        "request": "launch",
        "preLaunchTask": "npm: start",
        "url": "https://localhost:4200/#",
        "webRoot": "${workspaceFolder}/layon",
        "sourceMaps": true,
        "sourceMapPathOverrides": {
          "webpack:/*": "${webRoot}/*",
          "/./*": "${webRoot}/*",
          "/src/*": "${webRoot}/*",
          "/*": "*",
          "/./~/*": "${webRoot}/node_modules/*"
        }
      }
    ]
}